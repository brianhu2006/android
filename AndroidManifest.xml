<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.education101.math.twentyfour"
    android:versionCode="1"
    android:versionName="1.0">
	
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD"/>
	<uses-permission android:name="android.permission.RESTART_PACKAGES"/>
	<uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    
    <uses-sdk android:minSdkVersion="7" />

    <application
        android:icon="@drawable/app"
        android:label="@string/app_name" >
        
        <activity
            android:label="@string/app_name"
            android:name="com.education101.math.twentyfour.activity.AppActivity">
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity  
            android:label="@string/app_name"
            android:name="com.education101.math.twentyfour.activity.MainActivity"            
            android:screenOrientation="portrait">           
        </activity>
        
        <activity  
            android:label="@string/app_name"
            android:name="com.education101.math.twentyfour.activity.RankActivity"            
            android:screenOrientation="portrait">           
        </activity>
        
        <activity  
            android:label="@string/app_name"
            android:name="com.education101.math.twentyfour.activity.RankTimeActivity"            
            android:screenOrientation="portrait">           
        </activity> 
        
        <activity  
            android:label="@string/app_name"			android:name="com.education101.math.twentyfour.activity.RankCountActivity"            
            android:screenOrientation="portrait">           
        </activity>
    </application>

</manifest>